{
    "name": "stel-web",
    "version": "0.1.0",
    "author": "Samuel Gauvin-St-Pierre <samuel@sgstp.ca>",
    "private": false,
    "scripts": {
        "clean": "rimraf dist",
        "dev": "webpack-dev-server --config conf/webpack.config.js --content-base src --colors --port 5000 --history-api-fallback --client-log-level=error --mode=development",
        "dev--": "webpack-dev-server --config conf/webpack.config.js --content-base src --colors --host 192.168.0.170 --port 5000 --history-api-fallback --client-log-level=error --mode=development",
        "build": "webpack --env.prod --config conf/webpack.config.js --colors --optimize-minimize",
        "build-dev": "webpack --config conf/webpack.config.js --colors --optimize-minimize",
        "prod": "http-server dist -p 5000 -g -o",
        "tslint": "tslint -c conf/tslint.json -e 'src/**/*.d.ts' -e 'tests/unit/**/*.d.ts' -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts' 'tests/unit/**/*.ts' 'tests/e2e/*.ts'",
        "tslintfix": "tslint -c conf/tslint.json -e 'src/**/*.d.ts' -e 'tests/unit/**/*.d.ts' -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts' 'tests/unit/**/*.ts' 'tests/e2e/*.ts' --fix",
        "stylelint": "stylelint \"src/**/*.css\" \"src/**/*.scss\" --config conf/stylelint.json",
        "stylelinfix": "stylelint \"src/**/*.css\" \"src/**/*.scss\" --config conf/stylelint.json --fix",
        "tsc": "tsc --watch"
    },
    "dependencies": {
        "@types/lodash": "^4.14.120",
        "axios": "^0.18.0",
        "bootstrap": "^4.3.1",
        "moment": "^2.23.0",
        "vue": "^2.5.22",
        "vue-form-generator": "^2.3.3",
        "vue-router": "^3.0.2"
    },
    "devDependencies": {
        "@types/node": "^10.12.18",
        "awesome-typescript-loader": "^5.2.1",
        "compression-webpack-plugin": "^2.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^2.1.0",
        "custom-tslint-formatters": "^2.4.0",
        "es6-promise": "^4.2.6",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "http-server": "^0.11.1",
        "json-loader": "^0.5.7",
        "node-sass": "^4.11.0",
        "postcss-loader": "^3.0.0",
        "rimraf": "^2.6.3",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.10.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-scss": "^3.5.1",
        "stylelint-webpack-plugin": "^0.10.5",
        "svg-inline-loader": "^0.8.0",
        "tslint": "^5.12.1",
        "tslint-config-standard": "^8.0.1",
        "tslint-loader": "^3.5.4",
        "typescript": "^3.2.4",
        "uglify-js": "^3.4.9",
        "uglifyjs-webpack-plugin": "^2.1.1",
        "url-loader": "^1.1.2",
        "vue-property-decorator": "^7.3.0",
        "vue-template-compiler": "^2.5.22",
        "vue-template-loader": "^1.0.0",
        "webpack": "^4.28.4",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "license": "Apache-2.0"
}
